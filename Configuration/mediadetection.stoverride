name: 流媒体检测面板
desc: 检测流媒体解锁情况，分类显示解锁/未解锁和区域
tiles:
  - name: 流媒体检测
    icon: play.tv.fill
    backgroundColor: '#FF2D55'
    type: script
    label: 流媒体状态
    refreshInterval: 1800
script-providers:
  流媒体检测:
    url: https://raw.githubusercontent.com/xiaomingyve1/openclash/main/rule/streaming_check.js
    interval: 1800
    timeout: 10
    arguments:
      services: [chatgpt, youtube, netflix, disneyplus, tiktok, primevideo, hbomax]
      style: native
      colorLabel: true
